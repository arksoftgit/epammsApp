<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GraphicalLabelDesignerServlet</servlet-name>
        <servlet-class>com.ajax.GraphicalLabelDesignerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GraphicalLabelDesignerServlet</servlet-name>
        <url-pattern>/labeldesigner</url-pattern>
    </servlet-mapping>
    <servlet>
       <servlet-name>SortOrderServlet</servlet-name>
       <servlet-class>com.ajax.SortOrderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
       <servlet-name>SortOrderServlet</servlet-name>
       <url-pattern>/sortorder</url-pattern>
    </servlet-mapping>
    <servlet>
       <servlet-name>QueryDatabaseServlet</servlet-name>
       <servlet-class>com.ajax.QueryDatabaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
       <servlet-name>QueryDatabaseServlet</servlet-name>
       <url-pattern>/querydb</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
         30
      </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
