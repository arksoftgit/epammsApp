   /* fixes Firefox 0.9.3 */

   position: absolute;
   top: 98px;
   left: 0px;
   width: 1000px;
}

#navcontainer ul li
{
   display: inline;
   padding-left: 0;
   padding-right: 0;
   padding-bottom: 1px;  /* was 5px; */
