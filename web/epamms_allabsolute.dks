/************* TOP Navigation styles **************/

#mainnavigation
{
   font-family:Verdana, Arial, Helvetica, sans-serif;
   font-size: 12px;
   margin: 0;
   padding: 0px 0px 0px 0px; /* was padding: 3px 3px 3px 3px; */
   text-align:center;
   position: absolute;
   top:   126px; /* was 97px; ... 77px; */
   left: 0px;
   width: 1000px;
   line-height: 13px;
}

#mainnavigation ul
{
   border-top: 1px solid #000000;
   border-bottom: 1px solid #000000;
   border-right: 1px solid #000000;
   border-left:1px solid #000000;
   padding: 1px 0px 1px 0px;  /* was 3px 0px 3px 0px; */
   margin: 0;
   background-color:#cc0000;  /* red */
   color: white;  /* not in original */
   position: absolute;
   top: -3px;
   left: 0px;
   width: 1000px;
   line-height: 13px; /* height of menu bar; */
}

#mainnavigation ul li
{
   display: inline;
   padding: 3px 0px 3px 0px;  /* was 3px 3px 3px 3px; */
   margin: 0;
   width: 1000px;  /* was width:auto;
   left: 0px;  /* not in original */
}

#mainnavigation ul li a
{
   padding-left: 8px;  /* was 10px not in original; */
   padding-right: 8px;  /* was 10px not in original; */
   padding-bottom: 1px;  /* was 5px not in original; */
   padding-top: 1px;  /* was 5px not in original; */
   font-size: 11px;
   color: white;  /* was color: #ffffff; */
   background-color: #cc0000;  /* red */
   text-decoration: none;
   border-right: 1px solid #fff;  /* not in original */ /* padding: 3px 0px 3px 0px;   line-height: 13px; */
}

#mainnavigation ul li a:hover, #mainnavigation ul li a:focus
{
   background-color: #000000;
}

/********** NEW TOP NAVIGATION ****/

#navcontainer ul
{
   text-align: center;
   padding-bottom: 1px;  /* was 5px; */
   padding-top: 1px;  /* was 5px; */
   padding-left: 0;
   margin-top: 0;
   /* cancels gap caused by top padding in Opera 7.54 */
   margin-left: 0;
   background-color: #00aed6;
   color: white;

/* width: 100%; */
/* font-family: Arial,Helvetica,sans-serif; */
   font-family:Verdana, Arial, Helvetica, sans-serif;
