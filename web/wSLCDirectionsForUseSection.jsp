               strKey = Long.toString( lEKey );
               if ( StringUtils.equals( strKey, strEntityKey ) )
               {
                  // Stop while loop because we have positioned on the correct entity.
                  bFound = true;
               }
               else
                  csrRCk = mSubLC.cursor( "S_DirectionsForUseStatement" ).setNextContinue( );
            } // Grid
         }
      }

