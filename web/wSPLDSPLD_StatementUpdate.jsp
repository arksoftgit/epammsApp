      nRC = 0;
      VmlOperation.SetZeidonSessionAttribute( null, task, "wSPLDSPLD_StatementUpdate.jsp", "wSPLD.SPLIT_StatementForContinuation" );
         nOptRC = wSPLD.SPLIT_StatementForContinuation( new zVIEW( vKZXMLPGO ) );
