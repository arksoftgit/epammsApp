<tbody>

<%
try
{
   iTableRowCnt = 0;
   mSubLC = task.getViewByName( "mSubLC" );
   if ( VmlOperation.isValid( mSubLC ) )
   {
      long   lEntityKey;
      String strEntityKey;
      String strButtonName;
      String strOdd;
      String strTag;
      String strGridEditFirstAid;
      
      View vGridFirstAid;
      vGridFirstAid = mSubLC.newView( );
      csrRC2 = vGridFirstAid.cursor( "S_FirstAidStatement" ).setFirst(  );
      while ( csrRC2.isSet() )
      {
         strOdd = (iTableRowCnt % 2) != 0 ? " class='odd'" : "";
         iTableRowCnt++;

         lEntityKey = vGridFirstAid.cursor( "S_FirstAidStatement" ).getEntityKey( );
         strEntityKey = Long.toString( lEntityKey );
         strButtonName = "SelectButton" + strEntityKey;

         strGridEditFirstAid = "";
         nRC = vGridFirstAid.cursor( "S_FirstAidStatement" ).checkExistenceOfEntity( ).toInt();
         if ( nRC >= 0 )
         {
            strGridEditFirstAid = vGridFirstAid.cursor( "S_FirstAidStatement" ).getStringFromAttribute( "Text", "" );

            if ( strGridEditFirstAid == null )
               strGridEditFirstAid = "";
         }

         if ( StringUtils.isBlank( strGridEditFirstAid ) )
            strGridEditFirstAid = "&nbsp";

%>

<tr<%=strOdd%>>

   <td><%=strGridEditFirstAid%></td>

</tr>

<%
         csrRC2 = vGridFirstAid.cursor( "S_FirstAidStatement" ).setNextContinue( );
      }
      vGridFirstAid.drop( );
   }
}
catch (Exception e)
{
out.println("There is an error in grid: " + e.getMessage());
task.log().info( "*** Error in grid" + e.getMessage() );
}
%>
