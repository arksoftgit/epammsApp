   CursorResult csrRC = null;
   int    nMapError = 1;

   if ( webMapping == false )
      session.setAttribute( "ZeidonError", null );

   mSPLDef = task.getViewByName( "mSPLDef" );
   if ( VmlOperation.isValid( mSPLDef ) )
   {
      // ComboBox: IngredientsActive
      nRC = mSPLDef.cursor( "SPLD_IngredientsStatement" ).checkExistenceOfEntity( ).toInt();
      if ( nRC >= 0 )
      {
         strMapValue = request.getParameter( "hIngredientsActive" );
         try
         {
            if ( webMapping )
               VmlOperation.CreateMessage( task, "IngredientsActive", "", strMapValue );
            else
               mSPLDef.cursor( "SPLD_IngredientsStatement" ).setAttribute( "Active", strMapValue, "" );
         }
         catch ( InvalidAttributeValueException e )
         {
            nMapError = -16;
            VmlOperation.CreateMessage( task, "IngredientsActive", e.getReason( ), strMapValue );
         }
      }

      // EditBox: Prompt
      nRC = mSPLDef.cursor( "SPLD_IngredientsStatement" ).checkExistenceOfEntity( ).toInt();
      if ( nRC >= 0 ) // CursorResult.SET
      {
         strMapValue = request.getParameter( "Prompt" );
         try
         {
            if ( webMapping )
               VmlOperation.CreateMessage( task, "Prompt", "", strMapValue );
            else
               mSPLDef.cursor( "SPLD_IngredientsStatement" ).setAttribute( "Prompt", strMapValue, "" );
         }
         catch ( InvalidAttributeValueException e )
         {
            nMapError = -16;
            VmlOperation.CreateMessage( task, "Prompt", e.getReason( ), strMapValue );
         }
      }

      // EditBox: Percent
      nRC = mSPLDef.cursor( "SPLD_IngredientsStatement" ).checkExistenceOfEntity( ).toInt();
      if ( nRC >= 0 ) // CursorResult.SET
      {
         strMapValue = request.getParameter( "Percent" );
         try
         {
            if ( webMapping )
               VmlOperation.CreateMessage( task, "Percent", "", strMapValue );
            else
               mSPLDef.cursor( "SPLD_IngredientsStatement" ).setAttribute( "Percent", strMapValue, "" );
         }
         catch ( InvalidAttributeValueException e )
         {
            nMapError = -16;
            VmlOperation.CreateMessage( task, "Percent", e.getReason( ), strMapValue );
         }
      }

   }

   if ( webMapping == true )
      return 2;

   if ( nMapError < 0 )
      session.setAttribute( "ZeidonError", "Y" );

   return nMapError;
}

