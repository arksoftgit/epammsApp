      nRC = 1;  // do the redirection
      break;
   }

   while ( bDone == 0 && StringUtils.equals( strActionToProcess, "mTemplate" ) )
   {
      bDone = 1;
      session.setAttribute( "ZeidonAction", "mTemplate" );

      // Input Mapping
