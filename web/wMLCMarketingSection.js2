   }

   if ( nRC != 0 )
   {
      if ( nRC > 0 )
      {
         if ( nRC > 1 )
         {
            strURL = response.encodeRedirectURL( "wMLCMarketingSection.jsp" );
            task.log().debug( "Action Error Redirect to: " + strURL );
         }

         strActionToProcess = "";
         response.sendRedirect( strURL );
