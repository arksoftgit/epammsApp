      session.setAttribute( "ZeidonError", null );

   mMasLC = task.getViewByName( "mMasLC" );
   if ( VmlOperation.isValid( mMasLC ) )
   {
      // EditBox: ActiveTitle
      nRC = mMasLC.cursor( "M_IngredientsSection" ).checkExistenceOfEntity( ).toInt();
      if ( nRC >= 0 ) // CursorResult.SET
      {
         strMapValue = request.getParameter( "ActiveTitle" );
         try
         {
            if ( webMapping )
               VmlOperation.CreateMessage( task, "ActiveTitle", "", strMapValue );
            else
               mMasLC.cursor( "M_IngredientsSection" ).setAttribute( "ActiveTitle", strMapValue, "" );
         }
         catch ( InvalidAttributeValueException e )
         {
            nMapError = -16;
            VmlOperation.CreateMessage( task, "ActiveTitle", e.getReason( ), strMapValue );
         }
      }

      // EditBox: InertTitle
      nRC = mMasLC.cursor( "M_IngredientsSection" ).checkExistenceOfEntity( ).toInt();
      if ( nRC >= 0 ) // CursorResult.SET
      {
         strMapValue = request.getParameter( "InertTitle" );
         try
         {
            if ( webMapping )
               VmlOperation.CreateMessage( task, "InertTitle", "", strMapValue );
            else
