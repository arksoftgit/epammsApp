      szWriteBuffer = szWriteBuffer + " color=^" + szColor + "^" 
   END
   
   szFontFamily = mSPLDef.LLD_Block.FontFamily 
   IF szFontFamily != ""
      szWriteBuffer = szWriteBuffer + " font-family=^" + szFontFamily + "^" 
   END
   
   szFontSize = mSPLDef.LLD_Block.FontSize 
   IF szFontSize != ""
