/////////////////////////////////////////////////////////////////////////////
//
//  OPERATION: SetCursorNextWithData
//
/////////////////////////////////////////////////////////////////////////////
LOCAL OPERATION
SetCursorNextWithData( VIEW wWebXfer,
                       STRING ( 256 ) szText )

   INTEGER nRC

   nRC = zCURSOR_SET
   LOOP WHILE nRC >= zCURSOR_SET
      nRC = SetCursorNextEntity( wWebXfer, "Work", "" )
      IF nRC >= zCURSOR_SET
         GetStringFromAttribute( szText, wWebXfer, "Work", "Text" )
         IF szText != ""
            RETURN nRC
         END
      END
   END
   RETURN -1
END

