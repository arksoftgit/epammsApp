      StoreStringInRecord( mMasLC,
                           InternalEntityStructure,
                           InternalAttribStructure, szDisplayStatement )
      RETURN 0

        /* end zDERIVED_GET */
   OF   zDERIVED_SET:

        /* end zDERIVED_SET */
   END  /* case */

END


