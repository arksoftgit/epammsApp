/////////////////////////////////////////////////////////////////////////////
//
// OPERATION: SaveReusableBlock
//
/////////////////////////////////////////////////////////////////////////////
DIALOG OPERATION
SaveReusableBlock( VIEW ViewToWindow )
   VIEW wWebXfer REGISTERED AS wWebXfer
   VIEW mSubreg REGISTERED AS mSubreg
   VIEW qBlockRU BASED ON LOD qBlockRU
   INTEGER ID

   ID = mSubreg.ReusableBlockDefinition.ID

   ACTIVATE qBlockRU WHERE qBlockRU.Subregistrant.ID = mSubreg.Subregistrant.ID
   SET CURSOR FIRST qBlockRU.ReusableBlockDefinition WHERE qBlockRU.ReusableBlockDefinition.Name = wWebXfer.Root.SearchName
   IF RESULT >= zCURSOR_SET
      IF qBlockRU.ReusableBlockDefinition.ID != ID
         MessageSend( ViewToWindow, "", "Select Reusable Block",
                      "Reusable Block Name already exists",
                      zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
         DropObjectInstance( qBlockRU )
         RETURN 2
      END
      SET CURSOR NEXT qBlockRU.ReusableBlockDefinition WHERE qBlockRU.ReusableBlockDefinition.Name = wWebXfer.Root.SearchName
      IF RESULT >= zCURSOR_SET AND qBlockRU.ReusableBlockDefinition.ID != ID
         MessageSend( ViewToWindow, "", "Select Reusable Block",
                      "Reusable Block Name already exists",
                      zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
         DropObjectInstance( qBlockRU )
         RETURN 2
      END
   END
   mSubreg.ReusableBlockDefinition.Name = wWebXfer.Root.SearchName
// COMMIT mSubreg
END

/////////////////////////////////////////////////////////////////////////////
//
// OPERATION: ResumeEditingSPLD
//
/////////////////////////////////////////////////////////////////////////////
DIALOG OPERATION
ResumeEditingSPLD( VIEW ViewToWindow )

   VIEW qOrganiz REGISTERED AS qOrganizLogin
/*
   VIEW mSPLDef  BASED ON LOD mSPLDef
   VIEW mSubLC   BASED ON LOD mSubLC
   VIEW mSubProd BASED ON LOD mSubProd
   VIEW mSubreg  BASED ON LOD mSubreg
*/
   VIEW vTemp
   VIEW vQual
   VIEW mUser    BASED ON LOD mUser
   STRING ( 64 ) szLOD
   STRING ( 64 ) szEntityName
   STRING ( 64 ) szKeyAttributeName
   INTEGER lKeyAttributeValue
   STRING ( 64 ) szViewName

   ACTIVATE mUser WHERE mUser.User.ID = qOrganiz.User.ID
   SET CURSOR FIRST mUser.ResumePath WHERE mUser.ResumePath.DialogWindow = "wSPLDGraphicalView"
   IF RESULT >= zCURSOR_SET
      SET CURSOR FIRST mUser.ResumeObject
      LOOP WHILE RESULT >= zCURSOR_SET
         szLOD = mUser.ResumeObject.LOD
         szEntityName = mUser.ResumeObject.EntityName
         szKeyAttributeName = mUser.ResumeObject.KeyAttributeName
         lKeyAttributeValue = mUser.ResumeObject.KeyAttributeValue
         szViewName = mUser.ResumeObject.ViewName
         GET VIEW vTemp NAMED szViewName
         IF vTemp != 0
            DropView( vTemp )
         END
         BuildSimpleIntegerQualification( ViewToWindow, vQual, szEntityName, szKeyAttributeName, lKeyAttributeValue, "=" )
         ActivateObjectInstance( vTemp, szLOD, qOrganiz, vQual, zSINGLE )
         DropView( vQual )
         NAME VIEW vTemp szViewName
         IF szViewName = "mSPLDef"
            // Build the work components
            BuildCompositeEntriesForSPLD( vTemp )
         END
         SET CURSOR NEXT mUser.ResumeObject
      END
   ELSE
      MessageSend( ViewToWindow, "", "Resume",
                   "Resume to SPLD not available",
                   zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END

END

