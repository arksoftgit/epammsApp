            FOR EACH mLLD.LLD_SubBlock 
               CREATE ENTITY mSPLDef.LLD_SubBlock 
               SetMatchingAttributesByName( mSPLDef, "LLD_SubBlock", mLLD, "LLD_SubBlock", zSET_NULL )
