      MessageSend( ViewToWindow, "", "Copy Label",
                   "The Label Name must be unique.",
                   zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END


   ACTIVATE mSPLDefNew EMPTY
   NAME VIEW mSPLDefNew "NewSPLD"
   BuildSPLD_FromSPLD( mSPLDefNew, mSPLDef, mSubLC )
   
END
