            FOR EACH mLLD.LLD_SpecialSectionAttribute 
               CREATE ENTITY mSPLDef.LLD_SpecialSectionAttribute 
               SetMatchingAttributesByName( mSPLDef, "LLD_SpecialSectionAttribute", mLLD, "LLD_SpecialSectionAttribute", zSET_NULL )
