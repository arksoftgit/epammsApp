   // Reactivate the lSPLDLST object.
   SubregProductID = lSPLDLST.SubregProduct.ID 
   DropObjectInstance( lSPLDLST )
   ACTIVATE lSPLDLST Multiple WHERE lSPLDLST.SubregProduct.ID = SubregProductID
   NAME VIEW lSPLDLST "lSPLDLST"
   
END

